{
  "system_type": "TiDB",
  "version": "v6.1.0",
  "session": {
    "name": "{{ .SessionName }}",
    "description": "tune tidb cluster",
    "algorithm": "{{ .Algorithm }}",
    "target_objective": "{{ .Target }}",
    "more_is_better": {{ .MoreIsBetter }},
    "tuning_knobs": {
      {{ if eq (len .TuningKnobs) 0 }}
      "global": ["tidb.performance.gogc", "tikv.raft-engine.enable", "tidb_distsql_scan_concurrency"],
      {{ else }}
      "global": [{{ range $index, $value := .TuningKnobs}}{{ if eq $index 0}}"{{ $value }}"{{ else }}, "{{ $value }}"{{ end }}{{ end }}],
      {{ end }}
      "local": null
    },
    "hyper_parameters": null
  }
}